{
  "name": "tg-clicker",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev:web": "next dev --turbopack -p 3004 ",
    "dev:worker": "concurrently -n meditation,spiritPath -c blue,magenta \"tsx --watch workers/meditationWorker.ts\" \"tsx --watch workers/spiritPathWorker.ts\"",
    "dev": "concurrently -k -n web,worker -c blue,green \"npm run dev:web\" \"npm run dev:worker\"",
    "build": "next build && npm run build:workers",
    "build:workers": "rm -rf dist && tsx  scripts/build-workers.ts ",
    "start:workers": "concurrently -n meditation,spiritPath -c blue,magenta \"node dist/meditationWorker.js\" \"node dist/spiritPathWorker.js\"",
    "start": "concurrently \"npm run start:web\" \"npm run start:workers\"",
    "start:web": "next start -p 3004",
    "lint": "eslint",
    "lint:types": "tsc --noEmit",
    "prettier": "prettier -w .",
    "ngrok_tunel": "tsx scripts/ngrok.ts",
    "cloud_tunel": "cloudflared tunnel --url http://localhost:3004",
    "prepare": "git config core.hooksPath .git-hooks && chmod +x .git-hooks/pre-commit",
    "test": "jest --passWithNoTests",
    "seed": "tsx prisma/seed.ts",
    "reset": "docker compose down -v && docker compose up -d && prisma migrate reset --force && prisma migrate deploy && prisma generate"
  },
  "dependencies": {
    "@hookform/resolvers": "^5.2.2",
    "@prisma/adapter-pg": "^7.1.0",
    "@prisma/client": "^7.1.0",
    "@radix-ui/react-accordion": "^1.2.12",
    "@radix-ui/react-avatar": "^1.1.11",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-label": "^2.1.8",
    "@radix-ui/react-progress": "^1.1.8",
    "@radix-ui/react-radio-group": "^1.3.8",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-separator": "^1.1.8",
    "@radix-ui/react-slot": "^1.2.4",
    "@radix-ui/react-tabs": "^1.1.13",
    "@tanstack/react-query": "^5.90.12",
    "@telegram-apps/sdk": "^2.11.3",
    "@twa-dev/sdk": "^8.0.2",
    "amqplib": "^0.10.9",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "crypto": "^1.0.1",
    "dayjs": "^1.11.19",
    "dotenv": "^17.2.3",
    "glob": "^13.0.0",
    "ioredis": "^5.8.2",
    "jose": "^6.1.3",
    "lucide-react": "^0.556.0",
    "next": "^16.0.7",
    "next-themes": "^0.4.6",
    "react": "19.2.1",
    "react-dom": "19.2.1",
    "react-hook-form": "^7.68.0",
    "react-icons": "^5.5.0",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.4.0",
    "ts-node": "^10.9.2",
    "zod": "^4.1.13"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@faker-js/faker": "^10.1.0",
    "@tailwindcss/postcss": "^4",
    "@tanstack/eslint-plugin-query": "^5.91.2",
    "@tanstack/react-query-devtools": "^5.91.1",
    "@types/amqplib": "^0.10.8",
    "@types/node": "^24.10.1",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "concurrently": "^9.2.1",
    "esbuild": "^0.27.1",
    "eslint": "^9",
    "eslint-config-next": "16.0.7",
    "ngrok": "^5.0.0-beta.2",
    "prettier": "^3.7.4",
    "prisma": "^7.1.0",
    "tailwindcss": "^4",
    "tsx": "^4.21.0",
    "tw-animate-css": "^1.4.0",
    "typescript": "^5"
  }
}
