{
  "name": "tg-clicker",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "concurrently  -n WEB,WORKER -c cyan,green \"npm run dev:web\" \"npm run dev:worker\"",
    "dev:web": "next dev --turbopack",
    "dev:worker": "tsx workers/meditationWorker.ts",
    "build": "next build --turbopack && npm run build:worker",
    "build:worker": "tsc -p tsconfig.worker.json",
    "start": "concurrently  -n WEB,WORKER -c cyan,green \"npm run start:web\" \"npm run start:worker\"",
    "start:web": "next start -p 3004",
    "start:worker": "node dist/workers/meditationWorker.js",
    "lint": "eslint",
    "lint:types": "tsc --noEmit",
    "prettier": "prettier -w .",
    "ngrok": "tsx scripts/ngrok.ts",
    "prepare": "git config core.hooksPath .git-hooks && chmod +x .git-hooks/pre-commit",
    "test": "jest --passWithNoTests"
  },
  "dependencies": {
    "@hookform/resolvers": "^5.2.2",
    "@prisma/client": "^6.16.2",
    "@radix-ui/react-accordion": "^1.2.12",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-progress": "^1.1.7",
    "@radix-ui/react-radio-group": "^1.3.8",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-slot": "^1.2.3",
    "@tanstack/react-query": "^5.89.0",
    "@twa-dev/sdk": "^8.0.2",
    "bullmq": "^5.61.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "crypto": "^1.0.1",
    "dotenv": "^17.2.2",
    "ioredis": "^5.8.1",
    "jose": "^6.1.0",
    "lucide-react": "^0.544.0",
    "next": "15.5.2",
    "next-themes": "^0.4.6",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-hook-form": "^7.64.0",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.3.1",
    "ts-node": "^10.9.2",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@tanstack/eslint-plugin-query": "^5.89.0",
    "@types/jest": "^30.0.0",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "concurrently": "^9.2.1",
    "eslint": "^9",
    "eslint-config-next": "15.5.2",
    "jest": "^30.2.0",
    "ngrok": "^5.0.0-beta.2",
    "prettier": "^3.6.2",
    "prisma": "^6.16.2",
    "tailwindcss": "^4",
    "ts-jest": "^29.4.5",
    "tsx": "^4.20.6",
    "tw-animate-css": "^1.3.8",
    "typescript": "^5",
    "whatwg-fetch": "^3.6.20"
  }
}
